{  
    "builds": [
        {
            "src": "./api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
		{
			"src": "/api/(.*)",
			"dest": "/"
		}
	],
    "build": {
        "env": {
            "API_KEY": "@sm-api-key"
        }
    },
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
}
  